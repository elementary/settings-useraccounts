#: data/useraccounts.policy.in.in:10
msgid "Manage user accounts"
msgstr "Zarządzaj kontami użytkowników"

#: data/useraccounts.policy.in.in:11
msgid "Authentication is required to change user data"
msgstr "Wymagane jest uwierzytelnienie, aby zmienić dane użytkowników"

#: data/useraccounts.metainfo.xml.in:10
msgid "User Accounts Settings"
msgstr "Ustawienia kont użytkowników"

#: data/useraccounts.metainfo.xml.in:11
msgid ""
"Manage account permissions and configure user names, passwords, and photos"
msgstr ""
"Zarządzaj uprawnieniami konta i konfiguruj nazwy użytkowników, hasła oraz "
"zdjęcia"

#: data/useraccounts.metainfo.xml.in:26
msgid "elementary, Inc."
msgstr "elementary, Inc."

#: data/useraccounts.metainfo.xml.in:34 data/useraccounts.metainfo.xml.in:49
#: data/useraccounts.metainfo.xml.in:62 data/useraccounts.metainfo.xml.in:76
msgid "Improvements:"
msgstr "Ulepszenia:"

#: data/useraccounts.metainfo.xml.in:36
msgid "Improved screen reader accessibility"
msgstr "Poprawiono dostępność czytnika ekranu"

#: data/useraccounts.metainfo.xml.in:37 data/useraccounts.metainfo.xml.in:51
#: data/useraccounts.metainfo.xml.in:64 data/useraccounts.metainfo.xml.in:81
#: data/useraccounts.metainfo.xml.in:94
msgid "Updated translations"
